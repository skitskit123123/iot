#include<dht.h>
#include<liquidCrystal.h>
liquidCyrstal lcd(12,11,5,4,3,2);
#define dhtpin A0;
dht DHT;
void setup()
{
  Serial.begin(9600);
  lcd.begin(16,2);
}
void loop()
{
  int val=DHT.read11(dhtpin);
  int cel=DHT.temperature;
  int humi=DHT.humidity;
  lcd.print("temperature");
  lcd.print(cel);
  lcd.print((char)223);
  lcd.setCursor(0,1);
  lcd.print("humidity");
  lcd.print(humi);
  delay(1000);
  lcd.clear();
}
